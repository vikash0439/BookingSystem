<BorderPane prefHeight="800.0" prefWidth="1300.0" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.booking.controller.ReserveController">
   <top>
      <MenuBar BorderPane.alignment="TOP_LEFT">
         <menus>
            <Menu mnemonicParsing="false" onAction="#dashboard" text="Booking">
               <items>
                  <MenuItem mnemonicParsing="false" text="Dashboard" />
                  <MenuItem mnemonicParsing="false" onAction="#allcontract" text="Contracts" />
                  <MenuItem mnemonicParsing="false" onAction="#contract" text="New Contract" />
                  <MenuItem mnemonicParsing="false" onAction="#reserve" text="Reserved Dates" />
                  <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Accounting">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#receipt" text="Receipt" />
                  <MenuItem mnemonicParsing="false" onAction="#invoice" text="Invoice" />
                  <MenuItem mnemonicParsing="false" onAction="#tax" text="Tax" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Customer">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#customer" text="View" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Service">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#service" text="View" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Miscellanous">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#users" text="Users" />
                  <MenuItem mnemonicParsing="false" onAction="#slot" text="Slot" />
                  <MenuItem mnemonicParsing="false" onAction="#purpose" text="Purpose" />
                  <MenuItem mnemonicParsing="false" onAction="#statecode" text="State Code" />
                  <MenuItem mnemonicParsing="false" onAction="#reports" text="Reports" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <left>
      <VBox prefHeight="571.0" prefWidth="306.0" spacing="10.0" styleClass="background" stylesheets="@../styles/Styles.css" BorderPane.alignment="CENTER">
         <children>
            <Label text="Reservation" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" top="30.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="30.0" prefWidth="265.0">
               <children>
                  <Label text="Reserve ID - " textFill="#f8f7f7">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="ReserveID">
                     <HBox.margin>
                        <Insets left="25.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="25.0" />
               </VBox.margin>
            </HBox>
            <DatePicker fx:id="ServiceDate" prefHeight="31.0" prefWidth="249.0" promptText="Service Date">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin>
            </DatePicker>
            <ComboBox fx:id="Slot" prefHeight="31.0" prefWidth="247.0" promptText="Slot">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="ServiceTime" prefHeight="30.0" prefWidth="246.0" promptText="Service Time">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin></TextField>
            <ComboBox fx:id="ServiceID" prefHeight="31.0" prefWidth="246.0" promptText="Service Name">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="ReserveTitle" prefHeight="30.0" prefWidth="246.0" promptText="Event Name">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="InternalUsage" prefHeight="30.0" prefWidth="246.0" promptText="Internal Usage">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="30.0" prefWidth="250.0" spacing="10.0">
               <children>
                  <Button fx:id="reset" mnemonicParsing="false" onAction="#reset" prefHeight="30.0" prefWidth="75.0" style="-fx-background-color: white;" text="Reset">
                     <effect>
                        <DropShadow blurType="ONE_PASS_BOX" color="#1965d0" />
                     </effect>
                  </Button>
                  <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveService" prefHeight="30.0" prefWidth="75.0" styleClass="btnGreen" text="Save" textFill="WHITE">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <effect>
                        <DropShadow blurType="ONE_PASS_BOX" color="#6468b2" />
                     </effect>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="110.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </left>
   <center>
      <VBox alignment="TOP_RIGHT" prefHeight="571.0" prefWidth="692.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="41.0" prefWidth="640.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Reserves Services">
                     <HBox.margin>
                        <Insets left="10.0" right="450.0" top="7.0" />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="19.0" />
                     </font>
                  </Text>
                  <Button mnemonicParsing="false" onAction="#logout" prefHeight="22.0" prefWidth="70.0" styleClass="background" text="Logout" textFill="#fff9f9" textOverrun="CLIP">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </HBox.margin>
                     <effect>
                        <DropShadow blurType="ONE_PASS_BOX" color="#00000091" />
                     </effect>
                  </Button>
               </children>
            </HBox>
            <TableView fx:id="reservetable" prefHeight="810.0" prefWidth="959.0" tableMenuButtonVisible="true">
              <columns>
                 <TableColumn fx:id="colReserveID" maxWidth="700.0" prefWidth="20.0" text="Sr ." />
                 <TableColumn fx:id="colServiceDate" maxWidth="4000.0" prefWidth="75.0" text="Service Date" />
                  <TableColumn fx:id="colServiceTime" maxWidth="4000.0" prefWidth="75.0" text="Service Time" />
                  <TableColumn fx:id="colServiceID" maxWidth="4000.0" prefWidth="60.0" text="Service" />
                  <TableColumn fx:id="colReserveTitle" maxWidth="4000.0" prefWidth="60.0" text="Event Name" />
                  <TableColumn fx:id="colInternalUsage" maxWidth="4000.0" prefWidth="60.0" text="Internal Usage" />
              </columns>
               <VBox.margin>
                  <Insets left="10.0" right="5.0" />
               </VBox.margin>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
              
            </TableView>
         </children>
         <BorderPane.margin>
            <Insets left="25.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox prefHeight="60.0" prefWidth="1300.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
